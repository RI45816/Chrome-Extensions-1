{
	"name" : "Pin/Select Tabs",
	"version" : "2.0",
	"manifest_version" : 2,
	"key" : "",
	"update_url": "https://raw.githubusercontent.com/sirius16/Chrome-Extensions/master/update.xml",
	"description" : "Pin/Select",

	"permissions" : [
		"tabs",
		"<all_urls>"
	],

	"background" : {
		"scripts" : ["jquery-1.11.3.min.js", "background.js"],
		"persistent" : true
	},

	"commands" : {
		"1" : {
			"description" : "Pin Selected Tabs",
			"suggested_key" : {
				"default" : "Alt+Shift+P"
			}
		},
		"2" : {
			"description" : "Select All Tabs",
			"suggested_key" : {
				"default" : "Ctrl+Shift+P"
			}
		},
		"3" : {
			"description" : "Reload Selected Tabs",
			"suggested_key" : {
				"default" : "Ctrl+R"
			}
		}
	}
}
