{
	"name" : "Suspend Paste",
	"version" : "2.1",
	"manifest_version" : 2,
	"key" : "",
	"description" : "Suspend Paste",

	"permissions" : [
		"tabs",
		"<all_urls>",
		"clipboardRead"
	],

	"background" : {
		"scripts" : ["jquery-1.11.3.min.js", "background.js"],
		"persistent" : true
	},

	"commands" : {
		"pasting" : {
			"description" : "Suspended Paste",
			"suggested_key" : {
				"default" : "Ctrl+Shift+V"
			}
		}
	}
}
